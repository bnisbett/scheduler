<!DOCTYPE html>
<html>
<head>
  <title>Fullcalendar Rails3</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag "jquery","jquery-ui","fullcalendar","fullcalendar.min","gcal","jquery.rails","application" %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="user-nav">
			 <% if current_user %>
			  <%= link_to "Edit Profile", edit_user_path(:current) %> |
			  <%= link_to "Logout", logout_path %> |<%= current_user.login %>
			<% else %>
			  <%= link_to "Login", login_path %>
			<% end %>
	</div>

<%= yield %>

</body>
</html>
