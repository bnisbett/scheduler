<!DOCTYPE html>
<html>
<head>
  <title>Fullcalendar Rails3</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag "jquery","jquery-ui","fullcalendar","fullcalendar.min","gcal","jquery.rails","application" %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="user_nav">
			 <% if current_user %>
			  <%= link_to "Edit Profile", edit_user_path(:current) %> |
			  <%= link_to "Logout", logout_path %>
			<p><%= current_user.login %></p>
			<% else %>
			  <%= link_to "Register", new_user_path %> |
			  <%= link_to "Login", login_path %>

			<% end %>
	</div>
<span>
	<div id = 'loading'>
		Loading...
  </div>
</span>

<%= yield %>

</body>
</html>
